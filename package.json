{
  "name": "blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npx concurrently \"npm run api:dev\" \"npm run next:dev\"",
    "build": "npm run api:build && npx concurrently -ks first \"sleep 3 && npm run next:build\" \"npm run api:start\" && echo '_next' > out/.nojekyll",
    "next:dev": "next dev",
    "next:build": "next build && next export",
    "api:dev": "npx concurrently \"webpack -wc ./webpack-server.config.js\" \"sleep 3 && SHOW_DRAFTS=1 nodemon ./server/dist/server.js start\"",
    "api:build": "webpack -c ./webpack-server.config.js",
    "api:start": "node ./server/dist/server.js start"
  },
  "dependencies": {
    "@bem-react/classname": "^1.5.12",
    "@bem-react/di": "^3.1.1",
    "@mdx-js/loader": "^2.1.1",
    "@mdx-js/react": "^2.1.1",
    "@next/mdx": "^12.1.6",
    "@react-aria/interactions": "^3.8.4",
    "colors": "^1.4.0",
    "express": "^4.18.1",
    "feed": "^4.2.2",
    "glob": "^8.0.1",
    "glob-promise": "^4.2.2",
    "minimist": "^1.2.6",
    "next": "12.1.6",
    "next-global-css": "1.3.1",
    "next-mdx-remote": "^4.0.3",
    "node-watch": "^0.7.3",
    "query-string": "^7.1.1",
    "react": "^18.1.0",
    "react-dom": "18.1.0",
    "react-elegant-ui": "^0.3.0",
    "reading-time": "^1.5.0",
    "rehype-highlight": "^5.0.2",
    "rehype-stringify": "^9.0.3",
    "remark": "^14.0.2",
    "remark-gfm": "^3.0.1",
    "remark-rehype": "^10.1.0",
    "unist-util-visit": "^4.1.0"
  },
  "devDependencies": {
