    "@types/node": "17.0.31",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.3",
    "@types/watch": "^1.0.3",
    "concurrently": "^7.1.0",
    "esbuild-loader": "^2.18.0",
    "eslint": "8.14.0",
